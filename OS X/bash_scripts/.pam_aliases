echo "~/bash_scripts/.pam_aliases Loaded"

# Directory Shortcut
alias merser="cd ~/Projects/Client/GruppoPAM/merchandise_services"
alias merapp="cd ~/Projects/Client/GruppoPAM/merchandise_app"
alias dumps="cd ~/Projects/Client/GruppoPAM/Dumps"
alias cnmsclj="cd ~/Projects/Client/GruppoPAM/cnms_clj"

# Aliasing Log Tails
alias tomlog="tail -f /usr/local/Cellar/tomcat/7.0.34/libexec/logs/catalina.out"
alias clog="> /usr/local/Cellar/tomcat/7.0.34/libexec/logs/catalina.out"
alias dlog="tail -f /usr/local/var/postgres/server.log"
alias merlog="tail -f /data/services/logs/*.log"
alias mer2log="tail -f tmp/*.log"

# mvn aliases
alias rd="mvn tomcat:redeploy -DskipTests"
alias test="mvn db-migration:reset clean test -Ptest"
alias pack="mvn clean package -DskipTests -Pdev"

# Git Aliases
alias gs="git status"
alias gd="git diff"
alias gdc="git diff --cached"
alias gpr="git pull --rebase"

# Aliasing switch
alias nvm49="nvm use v0.4.9"

