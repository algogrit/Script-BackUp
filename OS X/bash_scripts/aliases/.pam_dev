echo -e "\033[1;36m~/bash_scripts/aliases/.pam Loaded\033[0m"

# Aliasing Log Tails
alias dlog="tail -f /usr/local/var/postgres/server.log"
alias servicelogs="tail -f /data/services/logs/*.log"
alias tmplog="tail -f tmp/*.log"
alias railslog="tail -f log/*.log"

# mvn aliases
alias devmigrate="mvn db-migration:migrate -Pdev"
alias devdbclean="mvn db-migration:drop db-migration:create db-migration:migrate -P dev"
alias devdbcreate="mvn db-migration:create db-migration:migrate -P dev"
alias ptest="mvn db-migration:reset clean test -Ptest"
alias pack="mvn clean package -DskipTests"
alias devpack="mvn clean package -DskipTests -Pdev"
alias jetstart="mvn jetty:run -Dconfig_file=file:/data/services/configurations/application.properties"

# Git Aliases
alias gs="git status"
alias gd="git diff"
alias gdc="git diff --cached"
alias gpr="git pull --rebase"
alias grlog="git log \`git rev-parse --abbrev-ref HEAD\`..origin/\`git rev-parse --abbrev-ref HEAD\`"

# Aliasing switch
alias nvm49="nvm use v0.4.9"

# Alias solr start
alias solr_start="solr /data/services/configurations/solr"
