{
  "editor.fontSize": 14,
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.renderWhitespace": "all",
  "editor.scrollBeyondLastLine": false,
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "[java]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true
  },
  "[go]": {
    "editor.tabSize": 4,
    "editor.insertSpaces": false
  },
  "[rust]": {
    "editor.tabSize": 4
  },
  "[python]": {
    "editor.tabSize": 4
  },
  "explorer.confirmDelete": false,
  "vs-kubernetes": {
    "vs-kubernetes.draft-path": "/Users/gaurav/.vs-kubernetes/tools/draft/darwin-amd64/draft",
    "vs-kubernetes.minikube-path": "/Users/gaurav/.vs-kubernetes/tools/minikube/darwin-amd64/minikube",
    "vs-kubernetes.helm-path.mac": "/Users/gaurav/.vs-kubernetes/tools/helm/darwin-amd64/helm",
    "vs-kubernetes.draft-path.mac": "/Users/gaurav/.vs-kubernetes/tools/draft/darwin-amd64/draft",
    "vs-kubernetes.kubectl-path.mac": "/Users/gaurav/.vs-kubernetes/tools/kubectl/kubectl",
    "vs-kubernetes.minikube-path.mac": "/Users/gaurav/.vs-kubernetes/tools/minikube/darwin-amd64/minikube"
  },
  "editor.wordWrap": "on",
  "files.autoSave": "off",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "java.home": "/Library/Java/JavaVirtualMachines/openjdk-14.0.2.jdk/Contents/Home",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "java.errors.incompleteClasspath.severity": "ignore",
  "python.jediEnabled": false,
  "terraform.indexing": {
    "enabled": false,
    "liveIndexing": false,
    "delay": 500,
    "exclude": [
      ".terraform/**/*",
      "**/.terraform/**/*"
    ]
  },
  "terraform.languageServer": {
    "external": true,
    "args": [
      "serve"
    ]
  },
  "cSpell.userWords": [
    "Kubernetes",
    "Twilio",
    "algogrit",
    "oauth"
  ],
  "C_Cpp.updateChannel": "Insiders",
  "go.formatTool": "goimports",
  "go.useLanguageServer": true,
  "go.testEnvVars": {
    "DEBUG": true,
    "DATABASE_URL": "postgres://gaurav@localhost:5432/roll-test?sslmode=disable",
    "DATABASE_DIALECT": "postgres"
  },
  "python.languageServer": "Microsoft",
  "java.semanticHighlighting.enabled": true,
  "java.refactor.renameFromFileExplorer": "autoApply",
  "workbench.editor.enablePreview": false,
  "workbench.settings.editor": "json",
  "mssql.connections": [
    {
      "server": "{{put-server-name-here}}",
      "database": "{{put-database-name-here}}",
      "user": "{{put-username-here}}",
      "password": "{{put-password-here}}"
    }
  ],
  "workbench.editor.untitled.hint": "hidden",
  "window.zoomLevel": 3
}
